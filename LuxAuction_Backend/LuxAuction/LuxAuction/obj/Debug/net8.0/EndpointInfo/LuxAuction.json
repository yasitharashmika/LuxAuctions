{
  "openapi": "3.0.4",
  "info": {
    "title": "LuxAuction API",
    "version": "v1"
  },
  "paths": {
    "/api/Auth/register": {
      "post": {
        "tags": [
          "Auth"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/RegisterDto"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/RegisterDto"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/RegisterDto"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Auth/login": {
      "post": {
        "tags": [
          "Auth"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/LoginDto"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/LoginDto"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/LoginDto"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Listings/create": {
      "post": {
        "tags": [
          "Listings"
        ],
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "required": [
                  "AuctionDuration",
                  "Category",
                  "Description",
                  "StartingBid",
                  "Title"
                ],
                "type": "object",
                "properties": {
                  "Title": {
                    "type": "string"
                  },
                  "Category": {
                    "type": "string"
                  },
                  "Description": {
                    "type": "string"
                  },
                  "StartingBid": {
                    "type": "number",
                    "format": "double"
                  },
                  "ReservePrice": {
                    "type": "number",
                    "format": "double"
                  },
                  "AuctionDuration": {
                    "type": "integer",
                    "format": "int32"
                  },
                  "Materials": {
                    "type": "array",
                    "items": {
                      "type": "string"
                    }
                  },
                  "Era": {
                    "type": "string"
                  },
                  "Condition": {
                    "type": "string"
                  },
                  "Weight": {
                    "type": "number",
                    "format": "double"
                  },
                  "Dimensions": {
                    "type": "string"
                  },
                  "Certificates": {
                    "type": "boolean"
                  },
                  "ShippingInfo": {
                    "type": "string"
                  },
                  "Images": {
                    "type": "array",
                    "items": {
                      "type": "string",
                      "format": "binary"
                    }
                  }
                }
              },
              "encoding": {
                "Title": {
                  "style": "form"
                },
                "Category": {
                  "style": "form"
                },
                "Description": {
                  "style": "form"
                },
                "StartingBid": {
                  "style": "form"
                },
                "ReservePrice": {
                  "style": "form"
                },
                "AuctionDuration": {
                  "style": "form"
                },
                "Materials": {
                  "style": "form"
                },
                "Era": {
                  "style": "form"
                },
                "Condition": {
                  "style": "form"
                },
                "Weight": {
                  "style": "form"
                },
                "Dimensions": {
                  "style": "form"
                },
                "Certificates": {
                  "style": "form"
                },
                "ShippingInfo": {
                  "style": "form"
                },
                "Images": {
                  "style": "form"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Listings/my-listings": {
      "get": {
        "tags": [
          "Listings"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Listings/active": {
      "get": {
        "tags": [
          "Listings"
        ],
        "parameters": [
          {
            "name": "search",
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "category",
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "price",
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "sort",
            "in": "query",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Listings/featured": {
      "get": {
        "tags": [
          "Listings"
        ],
        "parameters": [
          {
            "name": "count",
            "in": "query",
            "schema": {
              "type": "integer",
              "format": "int32",
              "default": 3
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Listings/{id}": {
      "delete": {
        "tags": [
          "Listings"
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "LoginDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "nullable": true
          },
          "password": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "RegisterDto": {
        "type": "object",
        "properties": {
          "fullName": {
            "type": "string",
            "nullable": true
          },
          "email": {
            "type": "string",
            "nullable": true
          },
          "role": {
            "type": "string",
            "nullable": true
          },
          "password": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    },
    "securitySchemes": {
      "Bearer": {
        "type": "http",
        "description": "JWT Authorization header using the Bearer scheme. Enter 'Bearer' [space] and then your token. Example: \"Authorization: Bearer {token}\"",
        "scheme": "Bearer"
      }
    }
  },
  "security": [
    {
      "Bearer": [ ]
    }
  ]
}